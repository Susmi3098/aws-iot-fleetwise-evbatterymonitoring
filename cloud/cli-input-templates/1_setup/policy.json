{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "timestreamIngestion",
      "Effect": "Allow",
      "Action": ["timestream:WriteRecords", "timestream:Select"],
      "Resource": "arn:aws:timestream:${AWS_REGION}:${AWS_ACCOUNT_ID}:database/FleetWiseDatabase/table/FleetWiseTable"
    },
    {
      "Sid": "timestreamDescribeEndpoint",
      "Effect": "Allow",
      "Action": ["timestream:DescribeEndpoints"],
      "Resource": "arn:aws:timestream:${AWS_REGION}:${AWS_ACCOUNT_ID}:database/FleetWiseDatabase"
    }
  ]
}
