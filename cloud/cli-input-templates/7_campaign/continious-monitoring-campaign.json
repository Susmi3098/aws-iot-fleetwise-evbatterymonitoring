{
  "name": "continious-monitoring-campaign",
  "targetArn": "arn:aws:iotfleetwise:${us-east-1}:${390403879653}:fleet/blog-fleet",
  "signalCatalogArn": "arn:aws:iotfleetwise:${us-east-1}:${390403879653}:signal-catalog/main-signal-catalog",
  "collectionScheme": {
    "timeBasedCollectionScheme": {
      "periodMs": 30000
    }
  },
  "dataDestinationConfigs":[
    {
      "timestreamConfig": {
	"executionRoleArn": "arn:aws:iam::${390403879653}:role/AWSIoTFleetWiseServiceRole",
	"timestreamTableArn": "arn:aws:timestream:${us-east-1}:${390403879653}:database/FleetWiseDatabase/table/FleetWiseTable"
      }
    }
  ],
  "diagnosticsMode": "OFF",
  "dataExtraDimensions": ["VehicleVIN"],
  "spoolingMode": "TO_DISK",
  "signalsToCollect": [
    {
      "name": "EVBatterySample.BMS.BatteryPack01.BatteryPackVoltagePlus",
      "maxSampleCount": 1
    },
    {
      "name": "EVBatterySample.BMS.BatteryPack01.BatteryPackVoltageMinus",
      "maxSampleCount": 1
    },

    {
      "name": "EVBatterySample.Drivetrain.ActualVehicleSpeed",
      "maxSampleCount": 1
    },
    {
      "name": "EVBatterySample.BMS.BatteryPack01.Cell001.CellTemperature",
      "maxSampleCount": 1
    },
    {
      "name": "EVBatterySample.BMS.BatteryPack01.ShuntPlusCurrent_a",
      "maxSampleCount": 1
    },
    {
      "name": "EVBatterySample.BMS.BatteryPack01.ShuntMinusCurrent_a",
      "maxSampleCount": 1
    },
    {
      "name": "EVBatterySample.BMS.BatteryPack01.ShuntPlusCurrentQualifier_enum",
      "maxSampleCount": 1
    },
    {
      "name": "EVBatterySample.BMS.BatteryPack01.ShuntMinusCurrentQualifier_enum",
      "maxSampleCount": 1
    }
  ]
}
